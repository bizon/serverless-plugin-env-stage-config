{
  "name": "serverless-plugin-env-stage-config",
  "version": "1.1.1",
  "author": "Bertrand Marron <bertrand.marron@gmail.com>",
  "description": "Serverless plugin to define environment variables files for stages",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "lint": "xo --version && xo"
  },
  "repository": "https://github.com/bizon/serverless-plugin-stage-config",
  "license": "MIT",
  "keywords": [
    "serverless",
    "serverless-plugin"
  ],
  "dependencies": {
    "@serverless/utils": "^5.6.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@bizon/semantic-release-config": "^1.0.1",
    "semantic-release": "^18.0.0",
    "xo": "^0.46.3"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "unicorn/prefer-module": "off"
    }
  },
  "release": {
    "extends": "@bizon/semantic-release-config"
  }
}
