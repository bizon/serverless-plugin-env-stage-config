{
  "name": "serverless-plugin-env-stage-config",
  "version": "1.1.1",
  "author": "Bertrand Marron <bertrand.marron@gmail.com>",
  "description": "Serverless plugin to define environment variables files for stages",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "lint": "xo --version && xo"
  },
  "repository": "https://github.com/bizon/serverless-plugin-stage-config",
  "license": "MIT",
  "keywords": [
    "serverless",
    "serverless-plugin"
  ],
  "dependencies": {
    "@serverless/utils": "^5.6.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "semantic-release": "^17.4.4",
    "xo": "^0.44.0"
  },
  "xo": {
    "semicolon": false,
    "space": 2
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.gitTag}"
        }
      ]
    ]
  }
}
